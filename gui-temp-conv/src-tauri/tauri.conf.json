{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gui-temp-conv",
  "version": "0.1.0",
  "identifier": "dev.bolorundurowb.gui-temp-conv",
  "build": {
    "beforeDevCommand": "yarn start",
    "devUrl": "http://localhost:8421",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist/gui-temp-conv/browser"
  },
  "app": {
    "windows": [
      {
        "title": "gui-temp-conv",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
